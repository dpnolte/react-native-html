{
  "name": "@react-native-html/node-parser",
  "version": "0.0.1",
  "description": "Using htmlparser2 to parse the nodes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "src/index.tsx",
  "author": "Derk Nolte",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@react-native-html/parser": "0.0.15",
    "htmlparser2": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4"
  },
  "scripts": {
    "build": "babel --extensions .ts,.tsx src --out-dir dist --ignore '**/__tests__/**' --source-maps --delete-dir-on-start && yarn declarations",
    "declarations": "tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "yarn test && yarn build",
    "test": "yarn jest ../parser"
  }
}
