{
  "name": "@react-native-html/parser",
  "version": "0.0.16",
  "description": "Parses html into data nodes that are structured for easy rendering in react native",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "react-native": "src/index.tsx",
  "author": "Derk Nolte",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "entities": "^2.0.0",
    "htmlparser2-without-node-native": "^3.9.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "htmlparser2": "^4.1.0"
  },
  "scripts": {
    "build": "babel --extensions .ts,.tsx src --out-dir dist --ignore '**/__tests__/**' --source-maps --delete-dir-on-start && yarn declarations",
    "declarations": "tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "yarn --cwd ../../ lint && yarn test && yarn --cwd ../node-parser test && yarn build",
    "test": "yarn jest"
  }
}
